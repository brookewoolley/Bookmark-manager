<!DOCTYPE html>
<html>
<head>
  </head>

  <body>

    <h1>Bookmarks</h1>
    <ul>
    <% @bookmarks.each do |bookmark| %>
    <li><a href="<%= bookmark.url %>">
      <%= bookmark.title %></a>
       <form action="/bookmarks/<%= bookmark.id %>/delete" method="post">
         <input type="submit" name="delete <% bookmark.title %>" value="x">
        </form>
       </li>
    <% end %>
    </ul>

  </body>
</html>
